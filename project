// project.cpp : This file contains the 'main' function. Program execution begins and ends there.
//
#include <iostream>
#include "Fill.h"
using namespace std;

int main()
{
    int choose;
    cout << "Enter size: 4x4-1, 5x5-2" << endl;
    cin >> choose;
    if (choose == 1) {
        FillField(4);  // 4x4
    }
    else if (choose == 2) {
        FillField(5);  // 5x5
    }

}
